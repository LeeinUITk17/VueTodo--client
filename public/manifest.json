{
  "manifest_version": 3,
  "name": "Vue Chrome Extension",
  "version": "1.0",
  "description": "A Chrome extension using Vue.js and TypeScript",
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "tabs", "activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
